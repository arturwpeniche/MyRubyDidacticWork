<h1>New user</h1>
<%= form_for User.new, :admin => false,  url: {action: :create} do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
<p>
    <%= f.label :password %><br>
    <%= f.text_area :pw %>
  </p>
 

  <p>
    <%= f.label :email %><br>
    <%= f.text_area :email %>
  </p>
 <p>
    <%= f.label :address %><br>
    <%= f.text_area :address %>
  </p>

  <p>
	
    <%= f.submit %>
  </p>
<% end %>


<%= link_to 'Back', welcome_index_path %>
